{
  "name": "PyChecker",
  "description": "Tracks changes to websites via PyChecker.com",
  "version": "0.1",
  "background": {
    "scripts": ["background.js"]
  },  
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
    "default_title": "Track Changes with PyChecker",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}
